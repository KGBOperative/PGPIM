<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.security.apps.pim"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="org.security.apps.pim.MainActivity"
            android:label="@string/title_activity_main" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.security.apps.pim.ContactListActivity"
            android:label="@string/title_activity_contact_list" >
        </activity>
        <activity
            android:name="org.security.apps.pim.NewContactActivity"
            android:label="@string/title_activity_new_contact" >
        </activity>
        <activity
            android:name="org.security.apps.pim.MessageWriter"
            android:label="@string/title_activity_message_writer" >
        </activity>
        <activity
            android:name="org.security.apps.pim.Encrypter"
            android:label="@string/title_activity_encrypter" >
        </activity>
    </application>

</manifest>